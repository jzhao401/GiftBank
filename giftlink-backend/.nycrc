{
  "all": true,
  "check-coverage": false,
  "lines": 80,
  "functions": 80,
  "branches": 80,
  "statements": 80,
  "exclude": [
    "node_modules/**",
    "test/**",
    "**/*.test.js",
    "coverage/**"
  ],
  "reporter": [
    "html",
    "text",
    "lcov",
    "json-summary"
  ],
  "report-dir": "./coverage"
}
